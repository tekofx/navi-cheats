% ssh


# See ssh version
ssh -v

# Generate public and private key
ssh-keygen

# Add private key
ssh-add <file>

# Execute ssh-agent
eval $(ssh-agent -s)

# Connect to host
ssh <user>@<ip_address>

# Connect to host by port
ssh -p <port> <user>@<ip_address>

# Enable X11 forwarding
ssh -X <host>

